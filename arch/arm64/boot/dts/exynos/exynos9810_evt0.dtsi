					<0x1A2E4484>,
					<0x1A2E44A4>,
					<0x1A2E44C4>,
					<0x1A2E44E4>;
			dma-instwrapper = <0x1A2E4500>;
			dma-mask-bit = <36>;
			coherent-mask-bit = <36>;
		};
	};

