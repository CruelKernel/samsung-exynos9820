					5
					0
					0
				>;
			};
			CLUSTER_COST_LIT: cluster-core0{
				busy-cost-data = <
					101	7
					151	8
					177	10
					203	11
					229	13
					265	15
					282	18
					308	22
					334	26
					367	30
					386	36
				>;
				idle-cost-data = <
					5
					5
					0
				>;
			};
			CLUSTER_COST_BIG: cluster-core1{
			busy-cost-data = <
					401	63
					445	70
					490	77
					534	85
