&pinctrl_0 {
	wpc_int: wpc-int {
		 samsung,pins = "gpa3-1";
		 samsung,pin-function = <0xf>;
		 samsung,pin-pud = <0>;
		 samsung,pin-drv = <0>;
	 };

	wpc_det: wpc-det {
		 samsung,pins = "gpa2-0";
		 samsung,pin-function = <0xf>;
		 samsung,pin-pud = <1>;
	 };
};

&pinctrl_5 {
	jig_gpio: jig-gpio {
