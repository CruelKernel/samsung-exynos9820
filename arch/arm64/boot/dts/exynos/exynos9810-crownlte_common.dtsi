				200 1690000 0 1248000 0 845000 400000 1 1 1
				300 1924000  0 1456000 0 1352000 534000 1 1 1
				>;
		};
		boot_device@7 {
			net_boost,label="P2P";
			net_boost,node="p2p-wlan0-0";
			net_boost,table_size = <3>;
			net_boost,table= <
