		reg = <0x10>; /* 1 bit right shift */
		id = <2>; /* matching fimc_is_sensor id */
		setfile = "default";
	};
};

/* PERI1 CAM3 */
&hsi2c_3 {
	#address-cells = <1>;
	#size-cells = <0>;
	gpios = <&gpc0 6 0 &gpc0 7 0>;
	status = "okay";
