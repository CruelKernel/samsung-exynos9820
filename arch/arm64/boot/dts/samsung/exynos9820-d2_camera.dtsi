	fimc-is-actuator@0C {
		compatible = "samsung,exynos5-fimc-is-actuator-ak737x";
		reg = <0x0C>;
		id = <0>; /* matching fimc_is_sensor id */
		/* AK7377: addr 0xF3, data 0x13 */
		vendor_product_id = <0xF3 0x13>;
		vendor_first_pos = <120>;
		vendor_first_delay = <2000>;
		vendor_use_sleep_mode;
		vendor_use_standby_mode; /* AK7377 with vendor_use_sleep_mode */
	};

	fimc-is-actuator@0F {
		compatible = "samsung,exynos5-fimc-is-actuator-ak737x";
		reg = <0x0F>;
		id = <2>; /* matching sensor id */
		/* AK7377: addr 0xF3, data 0x13 */
		vendor_product_id = <0xF3 0x13>;
		vendor_first_pos = <120>;
		vendor_first_delay = <2000>;
		vendor_soft_landing_list = <100 20 0 40>; /* 1st_pos delay 2nd_pos delay */
		vendor_use_sleep_mode;
		vendor_use_standby_mode; /* AK7377 with vendor_use_sleep_mode */
	};

	rear_eeprom@50 {
		compatible = "samsung,rear-eeprom-i2c";
		reg = <0x50>;

		rom_power_position = <0>;
		rom_size = <0x10000>;
		cal_map_es_version = <12>;
		camera_module_es_version = "A";
		/*skip_cal_loading;*/

		/* Davinci_Rear_Cal_Map_V012_20190214_for_ALL */
		/* 0 header */
		header_crc_check_list = <0x0000 0x01FB 0x01FC>;
		/* 0 master LSC F1.5 */
		/* 1 master LSC F1.8 */
		/* 2 master LSC F2.4 */
		/* 3 master all */
		crc_check_list = <0x0200 0x1BFB 0x1BFC
							0x1C00 0x35FB 0x35FC
							0x3600 0x4FFB 0x4FFC
							0x6500 0xAFDF 0xAFFC>;

		/* 0 u-wide dual cal data */
		/* 1 tele LSC */
		/* 2 tele dual cal data */
		/* wide-TOF-dual cal data */
		dual_crc_check_list = <0xB000 0xB8FB 0xB8FC
								0xB900 0xD2FB 0xD2FC
								0xDA00 0xF1DF 0xF1FC
								0xF200 0xFAFB 0xFAFC>;

		/* Master */
		rom_header_version_start_addr = <0x50>;
		rom_header_cal_data_start_addr = <0x200>;

		rom_header_cal_map_ver_start_addr = <0x70>;
		rom_header_isp_setfile_ver_start_addr = <0x74>;
		rom_header_project_name_start_addr = <0x7E>;

		rom_header_module_id_addr = <0xAE>; /* 0xA8 + reserved bytes */
		rom_header_sensor_id_addr = <0xB8>;

		rom_header_mtf_data_addr = <0xE0>;
		rom_header_f2_mtf_data_addr = <0x116>;
		rom_header_f3_mtf_data_addr = <0x14C>;

		rom_awb_master_addr = <0xAD60>;
		rom_awb_module_addr = <0xAD84>;

		rom_af_cal_macro_addr = <0xAD40>;
		rom_af_cal_d50_addr = <0xAD38>;
		rom_af_cal_pan_addr = <0xAD34>;

		/* Slave 0 */
		rom_header_sensor2_version_start_addr = <0x40>;
		rom_header_sensor2_id_addr = <0xC8>;
		rom_header_sensor2_mtf_data_addr = <0x182>;

		rom_sensor2_awb_master_addr = <0xE860>;
		rom_sensor2_awb_module_addr = <0xE884>;

		rom_sensor2_af_cal_d50_addr = <0xE838>;
		rom_sensor2_af_cal_pan_addr = <0xE834>;

		/* Dual cal slave 0 - Ultra wide */
		rom_dualcal_slave0_start_addr = <0xB000>;
		rom_dualcal_slave0_size = <0x800>; /* 2048 */
		/* x y z sx sy */
		/* range max_err avg_err dll_version*/
		rom_dualcal_slave0_tilt_list = <0xB060 0xB064 0xB068 0xB0C0 0xB0C4
										0xB7E0 0xB7E4 0xB7E8 0xB000>;

		/* Dual cal slave 1 - Tele */
		rom_dualcal_slave1_start_addr = <0xE910>;
		rom_dualcal_slave1_size = <0x800>; /* 2048 */
		/* x y z sx sy */
		/* range max_err avg_err dll_version*/
		rom_dualcal_slave1_tilt_list = <0xE970 0xE974 0xE978 0xE9D0 0xE9D4
										0xF0F0 0xF0F4 0xF0F8 0xE910>;

		/* sensor ois - xgg ygg xcoef ycoef xsupper ysupper calmark */
		/* sensor2 ois - xgg ygg xcoef ycoef xsupper ysupper calmark */
		rom_ois_list = <0xAEC0 0xAEC4 0xF025 0xF027 0xAEE8 0xAEEA 0xAEE0
						0xF130 0xF134 0xF029 0xF02B 0xF158 0xF15A 0xF150>;

		/* Dual cal slave 2 - TOF */
		rom_dualcal_slave2_start_addr = <0xF200>;
		rom_dualcal_slave2_size = <0x8FC>; /* 2300 */

		/* Dual cal slave 2 - wide - TOF tilt */
		/* x y z sx sy */
		/* range max_err avg_err dll_version*/
		rom_dualcal_slave2_tilt_list = <0xF26C 0xF270 0xF274 0xF5C0 0xF5C4
										0xF6E0 0xF6E4 0xF6E8 0xF200>;
		/* Dual cal slave 3 - ultra wide - TOF tilt */
		/* x y z sx sy */
		/* range max_err avg_err dll_version*/
		rom_dualcal_slave3_tilt_list = <0xF360 0xF364 0xF368 0xF7C8 0xF7CC
										0xF8E8 0xF8EC 0xF8F0 0xF200>;
	};
};

