	fimc-is-3p9@2D {
		compatible = "samsung,exynos5-fimc-is-cis-3p9";
		reg = <0x2D>; /* 1 bit right shift */
		id = <4>; /* matching fimc_is_sensor id */
		setfile = "setA";
