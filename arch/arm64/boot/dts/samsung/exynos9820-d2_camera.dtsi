#include <dt-bindings/camera/fimc_is.h>

/ {
	fragment@camera {
		target-path = "/";
		__overlay__ {
			/* REAR CAMERA */
			fimc_is_sensor_2l4: fimc-is_sensor_2l4@20 {
				compatible = "samsung,sensor-module-2l4";

				pinctrl-names = "pin0", "pin1", "pin2", "release";
				pinctrl-0 = <>;
				pinctrl-1 = <&sensor_mclk0_out>;
				pinctrl-2 = <&sensor_mclk0_fn>;
				pinctrl-3 = <>;

				position = <0>;	/* Rear:0. Front:1. Rear_sub:2. Secure:3. */
				id = <0>; /* fimc_is_sensor id */
				mclk_ch = <0>;
				use_mclk_share;
				sensor_i2c_ch = <0>; /* SENSOR_CONTROL_I2C0 */

				rom_type = <2>; /* ROM_TYPE_EEPROM */
				rom_id = <0>;
				rom_cal_index = <0>;

				gpio_mclk = <&gpc1 4 0x1>;
				gpio_reset = <&gpg2 6 0x1>; /* sensor reset */
				gpio_ois_reset = <&gpg0 6 0x1>; /* ois reset */

				status = "okay";

				af {
					product_name = <18>; /* ACTUATOR_NAME_AK737X : ak7371 or ak7374 */
					i2c_ch = <1>; /* SENSOR_CONTROL_I2C1 */
				};

				flash {
					product_name = <11>; /* FLASH_GPIO */
				};

				ois {
					product_name = <100>; /* NOTHING */
				};

				mcu {
					product_name = <1>; /* STM32 */
					i2c_addr = <0x51>;
					i2c_ch = <1>; /* SENSOR_CONTROL_I2C1 */
				};

				aperture {
					product_name = <100>; /* NOTHING */
				};
			};

			/* REAR2 CAMERA */
			fimc_is_sensor_3m5: fimc-is_sensor_3m5@5A {
				compatible = "samsung,sensor-module-3m5";

				pinctrl-names = "pin0", "pin1", "pin2", "release";
				pinctrl-0 = <>;
				pinctrl-1 = <&sensor_mclk0_out>;
				pinctrl-2 = <&sensor_mclk0_fn>;
				pinctrl-3 = <>;

				position = <2>; /* Rear:0. Front:1, Tele:2 */
				id = <2>; /* fimc_is_sensor id */
				mclk_ch = <0>;
				use_mclk_share;
				sensor_i2c_ch = <2>; /* SENSOR_CONTROL_I2C2 */

				rom_type = <2>; /* ROM_TYPE_EEPROM */
				rom_id = <0>;
				rom_cal_index = <1>;
				rom_dualcal_id = <0>;
				rom_dualcal_index = <1>;

				gpio_mclk = <&gpc1 4 0x1>; /* mclk shared with wide-cam */
				gpio_reset = <&gpg3 0 0x1>; /* sensor reset */
				gpio_ois_reset = <&gpg0 6 0x1>; /* ois reset */
				gpio_subcam_sel = <&gpg4 3 0x1>; /* sub cam sel */

				status = "okay";

				af {
					product_name = <18>; /* ACTUATOR_NAME_AK737X : ak7371 or ak7374 */
					i2c_ch = <3>; /* SENSOR_CONTROL_I2C3 */
				};

				flash {
					product_name = <11>; /* FLASH_GPIO */
				};

				preprocessor {
					product_name = <100>; /* NOTHING */
				};

				ois {
					product_name = <100>; /* NOTHING */
				};

				mcu {
					product_name = <1>; /* STM32 */
					i2c_addr = <0x51>;
					i2c_ch = <3>; /* SENSOR_CONTROL_I2C3 */
				};
			};

			/* Ultra Wide CAMERA */
			fimc_is_sensor_3p9: fimc-is_sensor_3p9@2D {
				compatible = "samsung,sensor-module-3p9";

				pinctrl-names = "pin0", "pin1", "pin2", "release";
				pinctrl-0 = <>;
				pinctrl-1 = <&sensor_mclk3_out>;
				pinctrl-2 = <&sensor_mclk3_fn>;
				pinctrl-3 = <>;

				position = <4>; /* Rear:0. Front:1, Tele:2 */
				id = <4>; /* fimc_is_sensor id */
				mclk_ch = <3>;
				sensor_i2c_ch = <4>; /* SENSOR_CONTROL_I2C4 */

				rom_type = <2>; /* ROM_TYPE_EEPROM */
				rom_id = <2>;
				rom_cal_index = <0>;
				rom_dualcal_id = <0>;
				rom_dualcal_index = <0>;

				gpio_mclk = <&gpc1 7 0x1>; /* mclk3 */
				gpio_reset = <&gpg0 3 0x1>; /* sensor reset */
				status = "okay";

				af {
					product_name = <100>; /* NOTHING */
				};

				flash {
					product_name = <11>; /* FLASH_GPIO */
				};

				preprocessor {
					product_name = <100>; /* NOTHING */
				};

				ois {
					product_name = <100>; /* NOTHING */
				};
			};

			fimc_is_flash_gpio: fimc-is-flash-gpio@0 {
				compatible = "samsung,sensor-flash-s2mpb02";
				id = <0 2 4>;
				status = "okay";

				torch-gpio = <&gpp3 1 0x1>;
				flash-gpio = <&gpg0 7 0x1>;
			};

			/* FRONT CAMERA */
			fimc_is_sensor_3j1: fimc-is_sensor_3j1@10 {
				compatible = "samsung,sensor-module-3j1";

				pinctrl-names = "pin0", "pin1", "pin2", "release";
				pinctrl-0 = <>;
				pinctrl-1 = <&sensor_mclk1_out>;
				pinctrl-2 = <&sensor_mclk1_fn>;
				pinctrl-3 = <>;

				position = <1>;	/* Rear:0. Front:1. Rear_sub:2. Secure:3. */
				id = <1>; /* fimc_is_sensor id */
				mclk_ch = <1>;
				sensor_i2c_ch = <4>; /* SENSOR_CONTROL_I2C4 */

				rom_type = <2>; /* ROM_TYPE_EEPROM */
				rom_id = <1>;
				rom_cal_index = <0>;

				gpio_mclk = <&gpc1 5 0x1>;
				gpio_reset = <&gpg2 5 0x1>; /* sensor reset */
				gpio_subcam_sel = <&gpg4 3 0x1>; /* sub cam sel */
				power_seq_id = <1>;
				status = "okay";

				af {
					product_name = <18>; /* ACTUATOR_NAME_AK737X */
					i2c_ch = <5>; /* SENSOR_CONTROL_I2C5 */
				};

				flash {
					product_name = <100>; /* NOTHING */
				};

				ois {
					product_name = <100>; /* NOTHING */
				};
			};

			/* Rear TOF */
			fimc_is_sensor_imx516_r: fimc-is_sensor_imx516_r@20 {
				compatible = "samsung,sensor-module-imx516";

				pinctrl-names = "pin0", "pin1", "pin2", "release";
				pinctrl-0 = <>;
				pinctrl-1 = <&sensor_mclk2_out>;
				pinctrl-2 = <&sensor_mclk2_fn>;
				pinctrl-3 = <>;

				position = <8>; /* Rear:0. Front:1, Tele:2 Front_sub:3 super Wide:4 rear TOF:8 front TOF:9*/
				id = <3>; /* fimc_is_sensor id */
				mclk_ch = <2>;
				sensor_i2c_ch = <6>; /* SENSOR_CONTROL_I2C6 */

				rom_type = <2>; /* ROM_TYPE_EEPROM */
				rom_id = <4>;
				rom_cal_index = <0>;
				rom_dualcal_id = <0>;
				rom_dualcal_index = <2>;

				gpio_mclk = <&gpc1 6 0x1>; /* mclk2 */
				gpio_reset = <&gpg2 3 0x1>; /* sensor reset */
				gpio_buck_en = <&gpd0 1 0x1>; /* isl91128 enable */
				status = "okay";

				af {
					product_name = <100>;  /* NOTHING */
				};

				flash {
					product_name = <100>;  /* NOTHING */
				};

				preprocessor {
					product_name = <100>; /* NOTHING */
				};

				ois {
					product_name = <100>; /* NOTHING */
				};
			};

			/* Rear TOF */
			fimc_is_sensor_imx316_r: fimc-is_sensor_imx316_r@20 {
				compatible = "samsung,sensor-module-imx316";

				pinctrl-names = "pin0", "pin1", "pin2", "release";
				pinctrl-0 = <>;
				pinctrl-1 = <&sensor_mclk2_out>;
				pinctrl-2 = <&sensor_mclk2_fn>;
				pinctrl-3 = <>;

				position = <8>; /* Rear:0. Front:1, Tele:2 Front_sub:3 super Wide:4 rear TOF:8 front TOF:9*/
				id = <3>; /* fimc_is_sensor id */
				mclk_ch = <2>;
				sensor_i2c_ch = <6>; /* SENSOR_CONTROL_I2C6 */

				rom_type = <2>; /* ROM_TYPE_EEPROM */
				rom_id = <4>;
				rom_cal_index = <0>;
				rom_dualcal_id = <0>;
				rom_dualcal_index = <2>;

				gpio_mclk = <&gpc1 6 0x1>; /* mclk2 */
				gpio_reset = <&gpg2 3 0x1>; /* sensor reset */
				gpio_buck_en = <&gpd0 1 0x1>; /* isl91128 enable */
				status = "okay";

				af {
					product_name = <100>;  /* NOTHING */
				};

				flash {
					product_name = <100>;  /* NOTHING */
				};

				preprocessor {
					product_name = <100>; /* NOTHING */
				};

				ois {
					product_name = <100>; /* NOTHING */
				};
			};
		}; /* end of __overlay__ */
	}; /* end of fragment */
}; /* end of root */

