	#address-cells = <1>;
	#size-cells = <0>;
	status = "okay";
	pinctrl-names = "default";
	pinctrl-0 = <&spi2_bus &spi2_cs_func>;

	cs47l93: cs47l93@0 {
		compatible = "cirrus,cs47l93";
		reg = <0x0>;

		spi-max-frequency = <25000000>;

		interrupts = <2 0 0>;
		interrupt-controller;
		#interrupt-cells = <2>;
		interrupt-parent = <&gpa2>;
		gpio-controller;
		#gpio-cells = <2>;
		#sound-dai-cells = <1>;

		AVDD-supply = <&ldo17_reg>;
		DBVDD1-supply = <&ldo17_reg>;
		CPVDD1-supply = <&ldo17_reg>;
		CPVDD2-supply = <&ldo18_reg>;
		DCVDD-supply = <&ldo18_reg>;

		reset-gpios = <&gpg1 0 0>;

		cirrus,dmic-ref = <
			MADERA_DMIC_REF_MICBIAS1 /* IN1 */
			MADERA_DMIC_REF_MICBIAS1 /* IN2 */
			MADERA_DMIC_REF_MICBIAS1 /* IN3 */
			MADERA_DMIC_REF_MICBIAS2 /* IN4 */
		>;
		cirrus,inmode = <
			MADERA_INMODE_DMIC MADERA_INMODE_DMIC /* IN1AL IN1AR */
			MADERA_INMODE_DIFF MADERA_INMODE_DIFF /* IN1BL IN1BR */
			MADERA_INMODE_DMIC MADERA_INMODE_DMIC /* IN2AL IN2AR */
			MADERA_INMODE_SE   MADERA_INMODE_SE   /* IN2BL IN2BR */
		>;

		pinctrl-names = "probe", "active";
		pinctrl-0 = <&codec_reset &codec_irq>;
		pinctrl-1 = <&cs47l93_defaults>;

		madera_pinctrl: madera-pinctrl {
			compatible = "cirrus,madera-pinctrl";

			cs47l93_defaults: cs47l93-gpio-defaults {
				aif1 {
					groups = "aif1";
					function = "aif1";
					bias-bus-hold;
				};
