&panel_0 {

    status = "okay";

	ddi_info = <&s6e3ha9_davinci>;

	#address-cells = <2>;
	#size-cells = <1>;
	ranges;

	gpios {
		disp-reset {
			name = "disp-reset";
			gpios = <&gpa3 4 1>;	/* ACTIVE_LOW */
			dir = <0>;  /* OUT */
			irq-type = <0x0>;   /* NONE */
		};

		disp-det {
			name = "disp-det";
			gpios = <&gpg2 2 1>;  /* 0:OK, 1:NOK(active) */
			dir = <1>;  /* IN */
			irq-type = <0x2>;   /* FALLING EDGE */
			irq-pend {
				reg = <0x0 0x10430A18 0x4>;
				bit = <0x2>;
			};
		};

		err-fg {
			name = "err-fg";
			gpios = <&gpa3 5 0>; 		/* 0:NOK(active) 1:OK */
			dir = <1>;  				/* IN */
			irq-type = <0x00002001>;	/* RISING EDGE | ONE_SHOT */
		};

		conn-det {
			name = "conn-det";
			gpios = <&gpm23 0 0>; 		/* 0:NOK(active) 1:OK */
			dir = <1>;  				/* IN */
			irq-type = <0x00002001>;	/* RISING EDGE | ONE_SHOT */
		};
	};

	regulators {
		ddi-3p0 {
			regulator = <&dp_ldo4>;
			def-voltage = <3000000>;
			lpm-voltage = <3000000>;
		};

		ddi-1p8 {
			regulator = <&dp_ldo1>;
			def-voltage = <1800000>;
			lpm-voltage = <1800000>;
		};

		ddr-1p6 {
			regulator = <&dp_buck>;
			def-voltage = <1600000>;
			lpm-voltage = <1500000>;
		};

		short-detect {
			regulator = <&dp_elvss>;
			def-current = <2000>;
			lpm-current = <8000>;
		};
	};

	panel-lookup {
		panel-name = "s6e3ha9_davinci2_a3_s0_default", "s6e3ha9_beyondx_a3_s0_default";
		panel-ddi-info = <&s6e3ha9_davinci &s6e3ha9_beyondx_df>;
		panel-lut = <
			0x000000 0x000400 0x1 0x1  /* S6E3HA9 1440x3040 TEMPORARY PANEL */

			/* default panel */
			0x000000 0x000000 0x0 0x0  /* S6E3HA9 1440x3040 PANEL */
		>;
	};
};

