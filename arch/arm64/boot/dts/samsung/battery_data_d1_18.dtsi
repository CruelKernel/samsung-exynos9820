#include "battery_data_d1_common.dtsi"

/ {
	fragment@battery {
		target-path = "/";
		__overlay__ {
				battery {
					battery,wpc_en = <&expander_gpios 18 0>;
					battery,mst_pwr_en = <&expander_gpios 5 0>;
				};
			}; /* end of __overlay__ */
	}; /* end of fragment */
};

&hsi2c_46 {
	mfc-charger@3b {
		pinctrl-0 = <&wpc_int &wpc_det>;

		battery,mst_pwr_en = <&expander_gpios 5 0>;
	};
};

&hsi2c_45 {
	pca9468@57 {
		pinctrl-0     = <&dc_irq>;

		pca9468,chg_gpio_en = <&expander_gpios 16 0>;
	};
};