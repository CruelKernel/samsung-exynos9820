						iod,io_type = <0>;		/* IODEV_MISC */
						iod,links = <0x800>;
						iod,attrs = <0x82>;		/* ATTR_SBD_IPC | ATTR_SIPC5 */
						iod,app = "Data Router";
						iod,ul_num_buffers = <16>;
