					io_device_0 {
						iod,name = "nr_cass";
						iod,id = <35>;
						iod,format = <1>;
						iod,io_type = <0>;
						iod,links = <0x800>;
						iod,attrs = <0x2082>;
						iod,max_tx_size = <2048>;
