#include "battery_data_d2_common.dtsi"
#include "battery_rx_data_lsi.dtsi"

/ {
};

&pinctrl_0 {
	 vrect_int: vrect-int {
		 samsung,pins = "gpa3-2";
		 samsung,pin-function = <0xf>;
		 samsung,pin-pud = <0>;
		 samsung,pin-drv = <0>;
	 };

	chg_gpio_en: chg-gpio-en {
		samsung,pins = "gpa4-1";
		samsung,pin-function = <1>;
		samsung,pin-pud = <0>;
		samsung,pin-drv = <0>;
	};

	pdet_b: pdet-b {
		 samsung,pins = "gpa2-1";
		 samsung,pin-function = <0xf>;
		 samsung,pin-pud = <0>;
		 samsung,pin-drv = <0>;
	 };
};

&pinctrl_5 {
	 wpc_en: wpc-en {
		 samsung,pins = "gpp1-7";
		 samsung,pin-function = <1>;
		 samsung,pin-pud = <0>;
		 samsung,pin-drv = <0>;
	 };

	 ping_nen: ping-nen {
		 samsung,pins = "gpp2-2";
		 samsung,pin-function = <1>;
		 samsung,pin-pud = <0>;
		 samsung,pin-drv = <0>;
	 };
};

&hsi2c_45 {
	pca9468@57 {
		pinctrl-0     = <&dc_irq &chg_gpio_en>;
	};
};

&hsi2c_46 {
	mfc-s2miw04-charger@3b {
		pinctrl-0 = <&wpc_int &wpc_det &pdet_b &wpc_en &ping_nen &vrect_int>;

		battery,vrect_int = <&gpa3 2 0>;
	};
};
