ifeq ($(CONFIG_MALI_DDK_VERSION),y)
	ifeq ($(CONFIG_MALI_BIFROST_R10P0),y)
		obj-y += b_r10p0/
	endif
	ifeq ($(CONFIG_MALI_BIFROST_R12P0),y)
		obj-y += b_r12p0/
	endif
	ifeq ($(CONFIG_MALI_BIFROST_R14P0),y)
		obj-y += b_r14p0/
	endif
	ifeq ($(CONFIG_MALI_BIFROST_R16P0),y)
		obj-y += b_r16p0/
	endif
	ifeq ($(CONFIG_MALI_BIFROST_R19P0_Q),y)
		obj-y += b_r19p0/
	endif
endif
